{
	"name": "fasta-validation",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"prebuild": "svelte-kit sync",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"test:fel-backend": "vitest run src/test/fel-backend.test.js --reporter=verbose",
		"test:slac-backend": "vitest run src/test/slac-backend.test.js --reporter=verbose",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"deploy": "wrangler pages deploy .svelte-kit/cloudflare",
		"wrangler:dev": "wrangler pages dev .svelte-kit/cloudflare"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^3.2.4",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
